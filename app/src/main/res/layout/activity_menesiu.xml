<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:exported="true"
    tools:context=".MenesiuActivity">

    <TextView
        android:id="@+id/Tekstas_Koks_Menuo"
        android:layout_width="232dp"
        android:layout_height="38dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="320dp"
        android:text="Atejau i pirma menesi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/Pasirinkimai_Spinners"
        android:layout_width="234dp"
        android:layout_height="27dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="88dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Button_Pasirinkti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="68dp"
        android:text="Button"
        app:layout_constraintStart_toEndOf="@+id/Pasirinkimai_Spinners"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>